@model TeacherSkillVm
<div class="card">
    <div class="card-body">
        <h4 class="card-title">@(((Teacher)ViewBag.Teacher).Fullname) add skill</h4>
        <form class="forms-sample" method="post" enctype="multipart/form-data">
          

            <div class="form-group">
                <label for="exampleInputPassword1">Skill Name</label>
                <input class="form-control" asp-for="Name" placeholder="Name">
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>



            <div class="form-group">
                <label for="exampleInputPassword1">Point(0-100)</label>
                <input class="form-control" asp-for="Point" placeholder="Point">
                <span asp-validation-for="Point" class="text-danger"></span>
            </div>

            <button type="submit" class="btn btn-primary mr-2">Create</button>
            <a class="btn btn-light" asp-action="Index">Cancel</a>
        </form>
    </div>
</div>